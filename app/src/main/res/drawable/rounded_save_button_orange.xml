<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="50dp"
        android:right="50dp"
        android:bottom="50dp"
        android:left="50dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/md_orange_A700"/>

        </shape>
    </item>
    <item
        android:top="25dp"
        android:right="25dp"
        android:bottom="25dp"
        android:left="25dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp"
                android:dashWidth="50px"
                android:dashGap="50px"
                android:color="@color/md_orange_A700"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:width="1dp"
                android:dashWidth="50px"
                android:dashGap="50px"
                android:color="@color/md_orange_A700"/>
        </shape>
    </item>
</layer-list>